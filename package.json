{
  "name": "findhelp",
  "version": "0.3.0",
  "description": "A simple and hackable lib to help create modular command line programs.",
  "main": "lib/index.js",
  "scripts": {
    "watch": "babel src --out-dir=lib --watch",
    "lint": "eslint src",
    "ava": "ava",
    "nyc": "nyc npm run ava",
    "test": "npm run lint && npm run nyc",
    "test-watch": "ava --watch",
    "prepublish": "babel src --out-dir=lib --ignore=*.test.js",
    "exe": "chmod +x lib/example.js"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vtex/findhelp.git"
  },
  "keywords": [
    "cli",
    "command",
    "argument",
    "args",
    "option",
    "parser",
    "commander",
    "yargs",
    "docopt",
    "neodoc",
    "vtex",
    "apps"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtex/findhelp/issues"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "es6-error": "^3.0.0",
    "pad": "^1.0.0",
    "ramda": "^0.21.0"
  },
  "devDependencies": {
    "ava": "^0.15.0",
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.21",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babel-register": "^6.9.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.10.2",
    "minimist": "^1.2.0",
    "nyc": "^6.4.4"
  },
  "ava": {
    "verbose": true,
    "failFast": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
